<%- include('blocks/header', {bot: bot, user: user, auth: auth}) %>

</head>
<link rel="stylesheet" type="text/css" href="public/servers/main.css">
<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/bulma/0.6.2/css/bulma.css">
<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.13/components/button.css">
<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.13/components/icon.css">
<meta name="theme-color" content="#23272A">
<meta property="og:title" content="Dark Bot">
<meta property="og:url" content="https://darkdashboard.glitch.me">
<meta property="og:description" content="Um Bot com várias funções, comandos e muitas opções, totalmente customizável">
<meta property="og:image" content="<%= bot.user.avatarURL %>?size=128">
<link rel="stylesheet" type="text/css" href="public/servers/main.css">
<style>    
.headerDiv {
        background-color: #18191C;
        width: 100%;
        height: 400px;
        margin-top: -40px;
        border-style: solid;
        border-width: 0px 0px 0.1px 0px;
        border-color: #7289DA;
    }

    .headerImage {
        margin-left: 100px;
    }

    .headerTextDescription {
        color: white;
        width: 640px;
        margin-left: 100px;
        margin-top: -40px;
        font-size: 17px;
        word-break: normal;
    }

    .textFeatured {
        color: #7289DA;
    }

    .text-center {
        font-size: 35px;
        color: white;
        margin-top: 10px;
        font-family: 'Calibri';
    }

    .list-users-icons {
        border-radius: 50%;
        width: 150px;
        height: 150px;
        margin-left: 100px;
    }

    .list-users-name {
        color: white;
        font-size: 20px;
        margin-left: 105px;
    }

    .list-users-status {
        width: 35px;
        height: 35px;
        border-radius: 50%;
        border-style: solid;
        border-width: 3px;
        border-color: #222222;
        margin-top: 110px;
        margin-left: -40px;
    }

    .bot {
        padding: 3px;
        width: 35px;
    }
</style>
</head>
<body>
    <div class="headerDiv">
        <img class="headerImage" src="https://eleven.sirv.com/ryouji/Banner_COnt.png">
        <p class="headerTextDescription">
            Aqui estão todos os contribuintes para o <span class="textFeatured">WebSite</span> e para o <span class="textFeatured">Dark</span>.
        </p>
    </div>
    <br>

    <h1 class="text-center" style="margin-left: -38px; margin-top: -15px;">
        Desenvolvedor
    </h1>
    <br>
    <div class="columns" id="Developers">
        <div class="is-one-quarter">
            <% let userDev1 = bot.users.get('473491713684668416'); %>
            <img class="list-users-icons" style="margin-left: 620px;" src="<%= userDev1.displayAvatarURL %>">
            <% if(userDev1.presence.status == 'online') { %>
            <img class="list-users-status" src="https://cdn.glitch.com/ba84bf22-2085-4b44-8650-b0acc3879203%2Fstatus-online.png">
            <% } else if(userDev1.presence.status == 'idle'){ %>
            <img class="list-users-status" src="https://cdn.glitch.com/ba84bf22-2085-4b44-8650-b0acc3879203%2Fstatus-idle.png">
            <% } else if(userDev1.presence.status == 'dnd') {%>
            <img class="list-users-status" src="https://cdn.glitch.com/ba84bf22-2085-4b44-8650-b0acc3879203%2Fstatus-dnd.png">
            <% } else { %>
            <img class="list-users-status" src="https://cdn.glitch.com/ba84bf22-2085-4b44-8650-b0acc3879203%2Fstatus-offline.png">
            <% } %>
            <p class="list-users-name" style="margin-left: 640px; margin-top: -130px;">
              <%= userDev1.tag%>
            </p>
        </div>
    </div>
    <br>
    <br>

    <h1 class="text-center" style="margin-left: -38px; margin-top: 5px;">
        Donaters
    </h1>
    <br>

    <div class="columns" id="Helpers">
        <div class="is-one-quarter">
            <% let userHelper2 = bot.users.get('428974077093609482'); %>
            <img class="list-users-icons" style="margin-left: 540px;" src="<%= userHelper2.displayAvatarURL %>">
            <% if(userHelper2.presence.status == 'online') { %>
            <img class="list-users-status" src="https://cdn.glitch.com/ba84bf22-2085-4b44-8650-b0acc3879203%2Fstatus-online.png">
            <% } else if(userHelper2.presence.status == 'idle'){ %>
            <img class="list-users-status" src="https://cdn.glitch.com/ba84bf22-2085-4b44-8650-b0acc3879203%2Fstatus-idle.png">
            <% } else if(userHelper2.presence.status == 'dnd') {%>
            <img class="list-users-status" src="https://cdn.glitch.com/ba84bf22-2085-4b44-8650-b0acc3879203%2Fstatus-dnd.png">
            <% } else { %>
            <img class="list-users-status" src="https://cdn.glitch.com/ba84bf22-2085-4b44-8650-b0acc3879203%2Fstatus-offline.png">
            <% } %>
            <p class="list-users-name" style="margin-left: 570px; margin-top: -134px;">
                <%= userHelper2.tag%>
            </p>
        </div>

        <div class="is-one-quarter">
            <% let userHelper3 = bot.users.get('465702855026802701'); %>
            <img class="list-users-icons" style="margin-left: 34px;" src="<%= userHelper3.displayAvatarURL %>">
            <% if(userHelper3.presence.status == 'online') { %>
            <img class="list-users-status" src="https://cdn.glitch.com/ba84bf22-2085-4b44-8650-b0acc3879203%2Fstatus-online.png">
            <% } else if(userHelper3.presence.status == 'idle'){ %>
            <img class="list-users-status" src="https://cdn.glitch.com/ba84bf22-2085-4b44-8650-b0acc3879203%2Fstatus-idle.png">
            <% } else if(userHelper3.presence.status == 'dnd') {%>
            <img class="list-users-status" src="https://cdn.glitch.com/ba84bf22-2085-4b44-8650-b0acc3879203%2Fstatus-dnd.png">
            <% } else { %>
            <img class="list-users-status" src="https://cdn.glitch.com/ba84bf22-2085-4b44-8650-b0acc3879203%2Fstatus-offline.png">
            <% } %>
            <p class="list-users-name" style="margin-left: 34px; margin-top: 12px;">
                <%= userHelper3.tag%>
            </p>
        </div>
    </div>
    <hr>
    <br>
    <br>
    <br>
</body>
